@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Sponsorship";
}
@model List<Core.Sponsorship>
<h2>Sponsorship</h2>
<br />
<table border="1" style="border-collapse:collapse;" width=100% cellpadding="4" cellspacing="20">
    <tr>
        <th>Название команды</th>
        <th>Название спонсора</th>
    </tr>
    @foreach (var ship in Model)
    {
        <tr>
            <td align="center">@ship.Command</td>
            <td align="center">@ship.SponsorName</td>
        </tr>
    }
</table>
