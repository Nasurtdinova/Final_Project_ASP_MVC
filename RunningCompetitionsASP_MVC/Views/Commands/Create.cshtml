@model CoreFramework.Command
@{
    ViewBag.Title = "Create";
}

<legend>Добавить изображение</legend>

<form asp-action="Create" asp-controller="Commands" method="post" enctype="multipart/form-data">
    <p>
        <label>Имя</label>
        <input name="FileName" class="form-control" />
    </p>
    <p>
        <label>Аватар</label>
        <input name="ImageData" type="file" class="form-control" />
    </p>
    <p>
        <input type="submit" value="Добавить" />
    </p>
</form>



<div>
    @Html.ActionLink("Вернуться к списку", "Add")
</div>


